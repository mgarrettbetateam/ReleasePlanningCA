# Environment Variables Template
# Copy this file to .env.local and update with your values
# Note: .env.local is in .gitignore and won't be committed

# API Configuration
VUE_APP_API_BASE_URL=https://your-api-server.com/api
VUE_APP_API_TIMEOUT=10000

# Widget Configuration
VUE_APP_WIDGET_TITLE=My Widget
VUE_APP_WIDGET_VERSION=1.0.0

# Feature Flags
VUE_APP_ENABLE_DEBUG=false
VUE_APP_ENABLE_MOCK_DATA=false

# Third-party Service Keys (add VUE_APP_ prefix to make them available in the app)
# VUE_APP_ANALYTICS_KEY=your-analytics-key
# VUE_APP_MAP_API_KEY=your-map-api-key
